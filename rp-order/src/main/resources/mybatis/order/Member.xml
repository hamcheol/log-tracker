<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rp.order.repository.MemberRepositoryImpl">
	<select id="selectMembers" parameterType="com.rp.order.model.Member" resultType="com.rp.order.model.Member">
		SELECT 
			mbr_id as mbrId,
			addr as addr
		FROM es_member
		<trim prefix="WHERE" prefixOverrides="AND|OR">
			<if test="mbrId != null">
			AND mbr_id = #{mbrId}
			</if>
		</trim>
	</select>
</mapper>