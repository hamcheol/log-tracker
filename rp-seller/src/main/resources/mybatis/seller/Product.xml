<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.rp.seller.repository.ProductRepositoryImpl">
	<insert id="insertProduct" parameterType="com.rp.seller.model.Product">
		INSERT INTO rp_product (
			id,
			name,
			price,
			seller_id,
		) values (
			#{id},
			#{name},
			#{price},
			#{seller.id}
		)
	</insert>
</mapper>